{
  "name": "gtelpay",
  "version": "0.0.2",
  "description": "Gtelpay: Api Report",
  "author": "VPA",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/database.config.ts",
    "typeorm:migrate": "npm run build && npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:create": "npm run typeorm migration:create -- -n"
  },
  "dependencies": {
    "@nestjs-architects/typed-cqrs": "^2.0.1",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "7.0.0",
    "@nestjs/websockets": "^10.3.10",
    "@node-saml/passport-saml": "^5.0.0",
    "asterisk-manager": "^0.2.0",
    "axios": "^1.8.4",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "configuration": "^3.1.1",
    "cookie-parser": "^1.4.6",
    "cron": "^3.1.7",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "ethereumjs-common": "^1.5.2",
    "ethereumjs-tx": "^2.1.2",
    "ethereumjs-util": "^7.1.5",
    "exceljs": "^4.4.0",
    "express-session": "^1.18.1",
    "form-data": "^4.0.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongodb": "^6.8.0",
    "mongoose": "^7.0.3",
    "nestjs-telegram": "^1.2.0",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.10.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-saml": "^3.2.4",
    "pg": "^8.10.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "selfsigned": "^2.4.1",
    "socket.io": "^4.7.5",
    "typeorm": "0.2",
    "web3": "^4.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.18.2",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.19.1",
    "@types/passport-saml": "^1.1.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^9.1.4",
    "jest": "29.5.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --check"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
